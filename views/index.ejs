<!doctype html>

<html lang="en">
  <meta charset="utf-8">

  <head>
    <title>Express</title>
    <link rel="stylesheet" href="public/stylesheets/style.css">
  </head>

  <body>
    <h1>Express</h1>
    <div id="display-table">
      <table>
        <% data.forEach(listing => { %>
          <tr>
            <td><%= listing.price %> <%= listing.timestamp %></td>
          </tr>
        <% }); %>
      </table>
    </div>
    <script src="public/javascripts/script.js"></script>
  </body>

</html>
